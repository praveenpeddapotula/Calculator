pipeline {
    agent any 
    tools {
        jdk 'JDK17'
        maven 'maven3'
    }
        stages {
            stage ('checkout') {
                steps {
                    echo "source code is here only"
                }
            }
            stage ('build') {
                steps {
                    echo "building artifacts using maven tool"
                    sh 'mvn clean package'
                }
            }
        }
}